# 클래스의 선언
malloc -> new : 생성자(== __intit__)
free -> delete : 소멸자
c++ = 큰int라고 생각

# private = class, public = struct

#include <iostream>

using namespace std;

class Cat{
    public:
    int itsAge;
    int itsWeight;
};

int main() {
    Cat Fristhy;
    Fristhy.itsAge = 5;
    cout<<"Fristhy is a cat who is";
    cout<<Fristhy.itsAge<<"years old.\n";
}
